//medium level
//in coding ninjas
string read(int n, vector<int> book, int target)
{
    // Write your code here.
    int i=0,j=0;
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(book[i]+book[j]==target)
            {
                return "YES";
            }
        }
    }
    return "NO";
}
