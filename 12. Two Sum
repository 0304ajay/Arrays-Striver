// in leetcode

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> a;
        int n=nums.size();
        int i=0,j=0;
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(nums[i]+nums[j]==target) 
            {
                a.push_back(i);
                a.push_back(j);
                return a;
            }
        }
    }
    return a;
    }
};
